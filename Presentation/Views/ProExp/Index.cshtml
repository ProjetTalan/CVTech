@model Presentation.ViewModels.ProExpViewModels.IndexProExpViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
		<th>
			@Html.DisplayNameFor(model => model.ProExpModels[0].CityName)
		</th>
        <th>
            @Html.DisplayNameFor(model => model.ProExpModels[0].CompanyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProExpModels[0].FromDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProExpModels[0].ToDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.ProExpModels) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FromDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ToDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
